    "graphql": "^14.0.2",
    "graphql-code-generator": "^0.12.1",
    "graphql-codegen-typescript-template": "^0.12.1",
    "graphql-tag": "^2.9.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  }
}
